{
  "name": "fidget-street",
  "version": "1.0.0",
  "description": "Fidget Street - Playful Fidget Toys & Stress Relief for All Ages",
  "author": "Fidget Street",
  "license": "MIT",
  "scripts": {
    "dev": "npx tailwindcss -i ./styles/tailwind.css -o ./styles/output.css --watch",
    "build": "npx tailwindcss -i ./styles/tailwind.css -o ./styles/output.css --minify",
    "preview": "npx serve .",
    "lint": "eslint scripts/**/*.js",
    "lint:fix": "eslint scripts/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,html,css,json,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@vitest/coverage-v8": "^4.0.16",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "jsdom": "^27.2.0",
    "netlify-cli": "^17.38.1",
    "playwright": "^1.57.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.4.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.958.0",
    "@supabase/supabase-js": "^2.39.0",
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "stripe": "^14.10.0"
  },
  "optionalDependencies": {
    "serve": "^14.2.0"
  },
  "keywords": [
    "fidget-toys",
    "sensory-toys",
    "stress-relief",
    "e-commerce",
    "adhd",
    "anxiety-relief"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "https://fidgetstreet.co.uk"
}
